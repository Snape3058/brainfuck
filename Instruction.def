#ifndef INSTRUCTION_KIND
#define INSTRUCTION_KIND(Name, Char)
#endif

#ifndef INSTRUCTION_CONSTANT
#define INSTRUCTION_CONSTANT(Name, Val)
#endif

INSTRUCTION_KIND(nop, '\0')
INSTRUCTION_KIND(add, '+')
INSTRUCTION_KIND(sub, '-')
INSTRUCTION_KIND(lsh, '<')
INSTRUCTION_KIND(rsh, '>')
INSTRUCTION_KIND(fjp, '[')
INSTRUCTION_KIND(bjp, ']')
INSTRUCTION_KIND(ipt, ',')
INSTRUCTION_KIND(opt, '.')

INSTRUCTION_CONSTANT(begin, nop)
INSTRUCTION_CONSTANT(end, opt)

#undef INSTRUCTION_KIND
#undef INSTRUCTION_CONSTANT
